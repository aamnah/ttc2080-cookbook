  "dependencies": {
    "@tailwindcss/vite": "^4.0.0-beta.5",
    "@types/express": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "mongoose": "^8.8.3",
    "tailwindcss": "^4.0.0-beta.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
